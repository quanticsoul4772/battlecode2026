# Battlecode 2026 Environment Configuration
# Copy this file to .env and customize as needed

# Team Configuration
TEAM_A=ratbot4
TEAM_B=examplefuncsplayer

# Map Selection
# Available maps: DefaultSmall, DefaultMedium, DefaultLarge
MAP=DefaultSmall

# Debug Options
DEBUG_MODE=false
SHOW_INDICATORS=true
OUTPUT_VERBOSE=true

# Build Options
VALIDATE_MAPS=true
ALTERNATE_ORDER=false

# Client Configuration
COMPATIBILITY_CLIENT=false

# Optional: GitHub CLI Token (if using gh commands for deployment stats)
# GITHUB_TOKEN=your_token_here

# Optional: Java Version
# JAVA_HOME=/path/to/java21
